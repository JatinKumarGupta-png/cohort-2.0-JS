function SaveConfirmation(title, msg) {
    var $confirm = $("#modalSaveConfirmation");
    $confirm.modal('show');
    $("#lblTitleSaveConfirmation").html(title);
    $("#lblMsgSaveConfirmation").html(msg);
}
